{
  "name": "mcu-debug-proxy",
  "version": "0.1.0",
  "preview": true,
  "description": "A debugger and debug adapter for MCUs",
  "keywords": [
    "MCU",
    "Cortex-Debug",
    "Mcu-Debug",
    "ARM",
    "RISC-V",
    "debugger"
  ],
  "homepage": "https://github.com/mcu-debug/mcu-debug#readme",
  "bugs": {
    "url": "https://github.com/mcu-debug/mcu-debug/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mcu-debug/mcu-debug.git"
  },
  "license": "MIT",
  "author": "Haneef Mohammed <haneefdm@gmail.com>",
  "extensionKind": [
    "ui"
  ],
  "activationEvents": [
    "onDebugResolve:cortex-debug",
    "onDebugResolve:mcu-debug",
    "onStartupFinished"
  ],
  "categories": [
    "Other"
  ],
  "engines": {
    "vscode": "^1.108.0"
  },
  "type": "commonjs",
  "main": "./dist/extension.js",
  "scripts": {
    "compile": "npm run check-types && node esbuild.js",
    "check-types": "tsc --noEmit",
    "watch": "npm-run-all -p watch:*",
    "watch:esbuild": "node esbuild.js --watch",
    "watch:tsc": "tsc --noEmit --watch --project tsconfig.json",
    "vscode:prepublish": "npm run package",
    "package": "npm run check-types && node esbuild.js --production"
  },
  "devDependencies": {
    "@types/hasbin": "^1.2.2",
    "@types/node": "^25.0.3",
    "@types/proper-lockfile": "^4.1.4",
    "@types/tmp": "^0.2.6",
    "@types/vscode": "^1.107.0",
    "@vscode/debugadapter": "^1.68.0",
    "@vscode/debugprotocol": "^1.68.0",
    "esbuild": "^0.27.2",
    "esbuild-node-externals": "^1.20.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^3.7.4",
    "@mcu-debug/shared": "*",
    "@vscode/extension-telemetry": "^1.2.0",
    "hasbin": "^1.2.3",
    "proper-lockfile": "^4.1.2",
    "tmp": "^0.2.5",
    "winston": "^3.19.0",
    "winston-vscode": "^1.0.0"
  }
}
